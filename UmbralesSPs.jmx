                <stringProp name="query">INSERT 
     INTO umbral_tipo_llamada
        ( `PLN_CLAVE`,`GRUPO_CIUDAD`,`GRUPO_PAIS`,`TLL_CLAVE`,`UMBRAL`, 
          `UMBRAL_EVENTO`,`TIPO_COBRO`,`STATUS_COBRO`,`POR_CUENTA`,`UMBRAL_LC`,
